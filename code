public int Fibonacci.of(int n) {
		if( n <= 0 ) {			
			 return -1;		
		}		
		if( n <= 2 ) {		
			 return 1;			
		}		
		int num1 = 1;
		int num2 = 1;
		int temp = 0;	
		if( n>2 ) {			
			 for( int i = 3 ; i <= n ; i++ ) {				
				 temp = num1 + num2;
				 num1 = num2;
				 num2 = temp;				
			}			
		}	
		 return temp;
	}
